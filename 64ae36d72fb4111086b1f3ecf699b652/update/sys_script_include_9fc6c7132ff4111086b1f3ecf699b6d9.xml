        <description>On demand script that allows to check if the email is correct</description>
        <name>validateEmailAddress</name>
        <script><![CDATA[function validateEmailAddress(email) {
	email = email + '';
	if(email.match(/^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/)) {
		return true;
	} else {
		return false;
	}
}]]></script>
